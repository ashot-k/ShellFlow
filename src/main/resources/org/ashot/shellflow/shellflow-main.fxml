<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="sceneContainer" prefHeight="750.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ashot.shellflow.Controller">
    <HBox alignment="CENTER_LEFT" fillHeight="false">
    </HBox>
    <StackPane VBox.vgrow="ALWAYS">
        <VBox VBox.vgrow="ALWAYS">
            <TabPane fx:id="mainTabPane" tabClosingPolicy="ALL_TABS" VBox.vgrow="ALWAYS">
            </TabPane>
        </VBox>
    </StackPane>
    <VBox>
      <Separator prefWidth="200.0" />
        <Text fx:id="fileLoaded" />
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" />
      </padding>
    </VBox>
</VBox>
